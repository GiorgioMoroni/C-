<Window x:Class="Esercitazione05_27_03_2024.Prezzi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Esercitazione05_27_03_2024"
        mc:Ignorable="d"
        Title="Prezzi" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <ToolBarTray DockPanel.Dock="Top">
                <ToolBar>
                    <Button x:Name="Home" Content="Home" />
                    <Button x:Name="Utenti" Content="Utenti"  />
                    <Button x:Name="Categorie" Content="Categorie" />

                    <Button x:Name="Prodotti" Content="Prodotti" />
                    <Button x:Name="Variazioni" Content="Variazioni" />
                    <Button x:Name="Ordini" Content="Ordini" />
                </ToolBar>
            </ToolBarTray>
            <TextBox AcceptsReturn="True" />
        </DockPanel>
    </Grid>
</Window>
